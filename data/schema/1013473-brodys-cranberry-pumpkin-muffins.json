{"@context": "http://schema.org", "@type": "Recipe", "name": "Brody\u2019s Cranberry-Pumpkin Muffins", "description": "", "author": {"@type": "Person", "name": "Jane E. Brody"}, "image": null, "totalTime": "PT40M", "recipeYield": "24 muffins", "recipeCuisine": "", "recipeCategory": "easy", "keywords": "dessert, healthy", "aggregateRating": {"@type": "AggregateRating", "ratingValue": 4, "ratingCount": 86}, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 145, "unsaturatedFatContent": "5 grams", "carbohydrateContent": "23 grams", "cholesterolContent": null, "fatContent": "5 grams", "fiberContent": "2 grams", "proteinContent": "3 grams", "saturatedFatContent": "1 gram", "sodiumContent": "75 milligrams", "sugarContent": "10 grams", "transFatContent": "0 grams"}, "recipeIngredient": ["1 1/2 cups whole-wheat flour", "1 1/2 cups all-purpose enriched flour", "1 cup sugar", "1 1/2 teaspoons cinnamon", "1 teaspoon baking powder", "1 teaspoon baking soda", "3/4 teaspoon allspice", "1/2 teaspoon salt (optional)", "1/2 cup canola oil", "2 1/2 cups canned pumpkin pur\u00e9e", "2 eggs, lightly beaten", "2 cups fresh cranberries, halved"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Heat oven to 350 degrees."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "In a large bowl, combine the flours, sugar, cinnamon, baking powder, baking soda, allspice and salt (if using) and mix well."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Add the oil, pumpkin and eggs and stir until just combined. Stir in the cranberries and spoon the batter into greased muffin cups."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Place the pans in the hot oven and bake for 40 minutes, or until a tester inserted into the center of the muffins comes out clean."}]}