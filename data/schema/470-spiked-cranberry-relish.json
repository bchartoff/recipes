{"@context": "http://schema.org", "@type": "Recipe", "name": "Spiked Cranberry Relish", "description": "", "author": {"@type": "Person", "name": "Marian Burros"}, "image": "https://static01.nyt.com/images/2014/04/10/dining/spikedcranberry/spikedcranberry-articleLarge.jpg", "totalTime": "PT20M", "recipeYield": "4 cups", "recipeCuisine": "", "recipeCategory": "quick, condiments", "keywords": "cranberry, grand marnier, fall", "aggregateRating": {"@type": "AggregateRating", "ratingValue": 4, "ratingCount": 15}, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 231, "unsaturatedFatContent": "8 grams", "carbohydrateContent": "33 grams", "cholesterolContent": null, "fatContent": "9 grams", "fiberContent": "5 grams", "proteinContent": "1 gram", "saturatedFatContent": "1 gram", "sodiumContent": "2 milligrams", "sugarContent": "25 grams", "transFatContent": null}, "recipeIngredient": ["1 pound fresh cranberries", "1/2 to 3/4 cup sugar", "1/4 cup orange juice", "6 tablespoons Grand Marnier", "2/3 cup coarsely chopped pecans"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Wash and pick over cranberries for foreign pieces. In heavy saucepan combine berries with sugar, 1/2 cup water and the orange juice. Bring to boil; reduce heat and cook at gentle boil until cranberries have popped. Do not wait until all pop, as the cranberries will become too soft."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Remove from heat; stir in Grand Marnier; cover well and chill."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "A couple of hours before serving, stir in pecans."}]}