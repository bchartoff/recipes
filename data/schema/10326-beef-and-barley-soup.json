{"@context": "http://schema.org", "@type": "Recipe", "name": "Beef And Barley Soup", "description": "", "author": {"@type": "Person", "name": "Marian Burros"}, "image": null, "totalTime": "PT3H", "recipeYield": "2 1/2 quarts", "recipeCuisine": "", "recipeCategory": "lunch, soups and stews", "keywords": "barley, beef, tomato, main course", "aggregateRating": {"@type": "AggregateRating", "ratingValue": 4, "ratingCount": 33}, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 46, "unsaturatedFatContent": "1 gram", "carbohydrateContent": "6 grams", "cholesterolContent": null, "fatContent": "1 gram", "fiberContent": "2 grams", "proteinContent": "4 grams", "saturatedFatContent": "0 grams", "sodiumContent": "241 milligrams", "sugarContent": "2 grams", "transFatContent": "0 grams"}, "recipeIngredient": ["4 cups canned tomatoes", "3/4 pound boneless chuck, cut in 1 1/2 inch cubes", "Salt and pepper to taste", "Celery tops from 1/2 bunch", "2 sprigs parsley", "1/4 cup regular barley", "1 cup tomato juice", "1/2 pound green beans, tips cut off, washed and cut in half", "1/2 cup coarsely chopped rutabaga", "1 1/2 cups coarsely chopped cabbage", "1/2 cup sliced carrots", "1/2 cup sliced celery", "1/2 cup thinly sliced onion"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Drain the liquid from the tomatoes and reserve. Add enough water to liquid to make one quart."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Place in large kettle with beef, salt and pepper to taste, celery tops and parsley. Cover and cook slowly for one hour."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Add barley. Cook one hour longer."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Remove and discard celery tops and parsley. Add tomato juice, reserved tomatoes and remaining ingredients."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Bring to a boil. Reduce heat and cook about 45 minutes."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Serve immediately, refrigerate or freeze. To serve reheat."}]}