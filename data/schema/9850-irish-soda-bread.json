{"@context": "http://schema.org", "@type": "Recipe", "name": "Irish Soda Bread", "description": "", "author": {"@type": "Person", "name": "Jeannette Ferrary"}, "image": null, "totalTime": "PT45M", "recipeYield": "Two loaves", "recipeCuisine": "irish", "recipeCategory": "project", "keywords": "buttermilk, soda, whole wheat flour, side dish, spring", "aggregateRating": {"@type": "AggregateRating", "ratingValue": 4, "ratingCount": 11}, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 256, "unsaturatedFatContent": "3 grams", "carbohydrateContent": "41 grams", "cholesterolContent": null, "fatContent": "7 grams", "fiberContent": "3 grams", "proteinContent": "8 grams", "saturatedFatContent": "4 grams", "sodiumContent": "261 milligrams", "sugarContent": "5 grams", "transFatContent": "0 grams"}, "recipeIngredient": ["2 cups whole-wheat flour", "2 cups unbleached white flour", "1 teaspoon baking soda", "3 teaspoons baking powder", "2 tablespoons sugar", "4 tablespoons butter or margarine, cut in small pieces, plus enough to grease the pans", "1 egg, beaten", "1 3/4 cups sour milk or buttermilk, approximately"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Preheat the oven to 375 degrees."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "In a food processor or large bowl, mix all dry ingredients together."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Add butter or margarine and mix until texture resembles coarse meal."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Add egg and enough milk to make a soft dough."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Knead or process until smooth."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Divide into two loaves. Place each in a greased 9-by-5-inch loaf pan, and bake for 35 to 45 minutes, or until golden."}]}