{"@context": "http://schema.org", "@type": "Recipe", "name": "Cranberry Corn Bread", "description": "", "author": {"@type": "Person", "name": "Joan Nathan"}, "image": null, "totalTime": "PT30M", "recipeYield": "12 servings", "recipeCuisine": "", "recipeCategory": "", "keywords": "cornmeal, cranberry, fall, side dish", "aggregateRating": {"@type": "AggregateRating", "ratingValue": 5, "ratingCount": 79}, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 157, "unsaturatedFatContent": "1 gram", "carbohydrateContent": "30 grams", "cholesterolContent": null, "fatContent": "3 grams", "fiberContent": "1 gram", "proteinContent": "3 grams", "saturatedFatContent": "2 grams", "sodiumContent": "159 milligrams", "sugarContent": "15 grams", "transFatContent": "0 grams"}, "recipeIngredient": ["2 tablespoons unsalted butter, melted, plus more to grease pan", "1 cup cranberries", "1/2 cup confectioners' sugar", "3/4 cup stone ground cornmeal", "1 cup all-purpose flour", "1/2 cup sugar", "3/4 teaspoon salt", "1 tablespoon baking powder", "1 cup milk", "1 large egg"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Heat oven to 400 degrees. Grease an 8-inch by 8-inch baking pan."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Cut each cranberry in half, place in large bowl with confectioners' sugar, and mix."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Sift in cornmeal, flour, sugar, salt, and baking powder, and mix."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Blend milk, egg, and melted butter in a bowl. Pour over cornmeal mixture and fold."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Pour into pan and bake for 20 minutes or until golden and a toothpick comes out clean when inserted."}]}