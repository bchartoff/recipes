{"@context": "http://schema.org", "@type": "Recipe", "name": "Curried Bulgur", "description": "", "author": {"@type": "Person", "name": "Marian Burros"}, "image": null, "totalTime": "PT20M", "recipeYield": "2 servings", "recipeCuisine": "indian", "recipeCategory": "dinner, weekday", "keywords": "bulgur, curry, peas, fall, side dish", "aggregateRating": {"@type": "AggregateRating", "ratingValue": 5, "ratingCount": 6}, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 307, "unsaturatedFatContent": "5 grams", "carbohydrateContent": "54 grams", "cholesterolContent": null, "fatContent": "7 grams", "fiberContent": "9 grams", "proteinContent": "11 grams", "saturatedFatContent": "1 gram", "sodiumContent": "220 milligrams", "sugarContent": "9 grams", "transFatContent": null}, "recipeIngredient": ["8 ounces whole onion or 7 ounces chopped onion (1 2/3 cups)", "2 teaspoons sesame oil", "1 teaspoon curry powder", "1/2 cup bulgur", "1 cup no-salt-added chicken stock", "1/2 cup frozen peas", "1/2 cup frozen corn"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Chop onion."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Heat oil in nonstick skillet or pot and saute onion until it begins to soften. Stir in curry powder and bulgur, mixing for about 30 seconds."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Add the stock. Stir, reduce heat, and simmer about 10 minutes."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "A few minutes before the bulgur is ready, stir in peas and corn and cook until they are heated through and bulgur is done."}]}