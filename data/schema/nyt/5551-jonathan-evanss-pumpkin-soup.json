{"@context": "http://schema.org", "@type": "Recipe", "name": "Jonathan Evans's Pumpkin Soup", "description": "", "author": {"@type": "Person", "name": "Nancy Arum"}, "image": null, "totalTime": "PT20M", "recipeYield": "12 servings", "recipeCuisine": "", "recipeCategory": "soups and stews", "keywords": "appetizer, side dish, milk, pumpkin, vegetarian", "aggregateRating": null, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 949, "unsaturatedFatContent": "21 grams", "carbohydrateContent": "16 grams", "cholesterolContent": null, "fatContent": "78 grams", "fiberContent": "4 grams", "proteinContent": "48 grams", "saturatedFatContent": "46 grams", "sodiumContent": "1242 milligrams", "sugarContent": "4 grams", "transFatContent": "2 grams"}, "recipeIngredient": ["1 onion, minced", "1 stalk celery, chopped", "6 tablespoons butter", "2 teaspoons salt", "1 teaspoon white pepper", "5 pounds cheese", "pumpkin (eight cups pulp)", "2 cups milk and freshly grated nutmeg to taste"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Steam pumpkin."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Saute onions and celery in butter until translucent."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Combine pumpkin pulp and sauteed onions and celery and puree in food processor or electric blender."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Combine pumpkin puree and milk and heat over low flame. Add salt and pepper."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Do not allow soup to boil. Serve with freshly grated nutmeg floating atop."}]}