{"@context": "http://schema.org", "@type": "Recipe", "name": "Yvonne Young Tarr's Sun-Dried Tomato Chutney", "description": "", "author": {"@type": "Person", "name": "Florence Fabricant"}, "image": null, "totalTime": "PT50M", "recipeYield": "2 pints", "recipeCuisine": "", "recipeCategory": "condiments", "keywords": "dried currants, spices, tomato", "aggregateRating": null, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 280, "unsaturatedFatContent": "0 grams", "carbohydrateContent": "70 grams", "cholesterolContent": null, "fatContent": "1 gram", "fiberContent": "3 grams", "proteinContent": "3 grams", "saturatedFatContent": "0 grams", "sodiumContent": "173 milligrams", "sugarContent": "64 grams", "transFatContent": "0 grams"}, "recipeIngredient": ["10 medium-size ripe fresh tomatoes, peeled, seeded and coarsely chopped", "1/2 cup cider vinegar", "1/2 cup dried currants", "1 large onion, peeled and chopped", "3 large cloves garlic, peeled and minced", "1 2-inch piece of fresh ginger, peeled and minced", "1 cup granulated sugar", "1 cup light brown sugar, packed", "1 teaspoon dry mustard", "1 teaspoon curry powder", "1/2 teaspoon ground cloves", "1/2 teaspoon salt", "1/2 cup coarsely chopped sun-dried tomatoes"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Cook tomatoes in half the vinegar until they have softened."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Add all the remaining ingredients except sun-dried tomatoes and simmer mixture until it has thickened, about 25 minutes."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Cool to room temperature, then fold in the sun-dried tomatoes. Store in the refrigerator."}]}