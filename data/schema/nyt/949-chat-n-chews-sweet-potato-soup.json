{"@context": "http://schema.org", "@type": "Recipe", "name": "Chat \u2018n\u2019 Chew\u2019s Sweet Potato Soup", "description": "", "author": {"@type": "Person", "name": "Yanick Rice Lamb"}, "image": null, "totalTime": "PT20M", "recipeYield": "6 servings", "recipeCuisine": "", "recipeCategory": "soups and stews", "keywords": "carrot, sweet potato, vegetarian", "aggregateRating": null, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 330, "unsaturatedFatContent": "7 grams", "carbohydrateContent": "37 grams", "cholesterolContent": null, "fatContent": "19 grams", "fiberContent": "7 grams", "proteinContent": "5 grams", "saturatedFatContent": "10 grams", "sodiumContent": "723 milligrams", "sugarContent": "13 grams", "transFatContent": null}, "recipeIngredient": ["4 large sweet potatoes", "8 carrots", "1 large onion, chopped", "1 tablespoon olive oil", "2 teaspoons grated fresh ginger", "1 teaspoon ground nutmeg", "1 tablespoon salt", "2 tablespoons fresh ground pepper", "1 cup heavy cream", "1 cup milk", "1/2 red bell pepper, minced", "3 sprigs parsley, chopped"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "In a large pot, boil sweet potatoes and carrots until very soft. Drain, peel and cube."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Saute onions in oil until soft."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Put sweet potatoes, onion, carrots and spices in food processor, and puree until slightly chunky."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Add cream and milk until desired consistency is reached."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "If desired, simmer over low heat for 5 minutes."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Garnish with bell pepper and parsley."}]}