{"@context": "http://schema.org", "@type": "Recipe", "name": "Sybil Arant's Catfish Baked With Cheese", "description": "", "author": {"@type": "Person", "name": "Craig Claiborne"}, "image": null, "totalTime": "PT30M", "recipeYield": "6 to 8 servings", "recipeCuisine": "", "recipeCategory": "dinner, easy, quick", "keywords": "catfish, fish, parmesan, main course", "aggregateRating": {"@type": "AggregateRating", "ratingValue": 5, "ratingCount": 17}, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 255, "unsaturatedFatContent": "8 grams", "carbohydrateContent": "4 grams", "cholesterolContent": null, "fatContent": "16 grams", "fiberContent": "1 gram", "proteinContent": "22 grams", "saturatedFatContent": "7 grams", "sodiumContent": "331 milligrams", "sugarContent": "0 grams", "transFatContent": "0 grams"}, "recipeIngredient": ["6 to 8 catfish fillets, about 2 pounds", "1/2 cup freshly grated Parmesan cheese", "1/4 cup flour", "Salt to taste, if desired", "Freshly ground pepper to taste", "1 teaspoon paprika", "1 egg, lightly beaten", "1 tablespoon milk", "1/4 cup melted butter", "1/4 cup sliced almonds"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Preheat the oven to 350 degrees."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Wipe the catfish dry."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Blend together the cheese, flour, salt, pepper and paprika."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Combine the egg and milk in a flat dish."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Dip the fillets in the egg mixture and then coat with the cheese mixture. Arrange the fillets in one layer in a baking dish and pour the butter over all. Sprinkle with the almonds. Place in the oven and bake 20 minutes."}]}