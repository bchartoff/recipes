{"@context": "http://schema.org", "@type": "Recipe", "name": "Herring Savory", "description": "", "author": {"@type": "Person", "name": "Robert Farrar Capon"}, "image": null, "totalTime": "PT12H15M", "recipeYield": "", "recipeCuisine": "scandinavian", "recipeCategory": "", "keywords": "bread crumbs, salt herring, side dish, winter", "aggregateRating": null, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 217, "unsaturatedFatContent": "7 grams", "carbohydrateContent": "5 grams", "cholesterolContent": null, "fatContent": "17 grams", "fiberContent": "0 grams", "proteinContent": "12 grams", "saturatedFatContent": "8 grams", "sodiumContent": "105 milligrams", "sugarContent": "1 gram", "transFatContent": "0 grams"}, "recipeIngredient": ["2 salt herrings", "1/3 cup dry bread crumbs", "3 tablespoons butter", "1/3 cup cream", "1 tablespoon chopped chives"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Soak whole salt herrings for 12 hours in several changes of cold water. Remove from water and bone and skin them, producing 4 fillets."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Preheat oven to 400 degrees."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Coat fillets thoroughly with bread crumbs. Butter small au gratin dish with 1 tablespoon butter, lay on fillets and dot with remaining butter."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Pour cream over and bake 5 to 10 minutes."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Sprinkle with chopped chives before serving."}]}