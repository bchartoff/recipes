{"@context": "http://schema.org", "@type": "Recipe", "name": "Rack of Lamb", "description": "", "author": {"@type": "Person", "name": "Barbara Kafka"}, "image": null, "totalTime": "PT30M", "recipeYield": "8 servings", "recipeCuisine": "jewish", "recipeCategory": "dinner, roasts", "keywords": "kosher, lamb, matzo meal, mustard, main course, spring", "aggregateRating": {"@type": "AggregateRating", "ratingValue": 4, "ratingCount": 25}, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 161, "unsaturatedFatContent": "8 grams", "carbohydrateContent": "5 grams", "cholesterolContent": null, "fatContent": "14 grams", "fiberContent": "0 grams", "proteinContent": "5 grams", "saturatedFatContent": "5 grams", "sodiumContent": "94 milligrams", "sugarContent": "0 grams", "transFatContent": "0 grams"}, "recipeIngredient": ["2 racks of lamb, fat removed above the eye", "2 tablespoons vegetable oil", "4 teaspoons coarse-grain mustard", "1 tablespoon dried rosemary, crushed", "1/2 teaspoon kosher salt", "1/2 teaspoon freshly ground black pepper", "3/4 cups matzoh meal"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "Preheat conventional oven to 500 degrees."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Place racks in roasting pan so that only the bones overlap."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Whisk oil, mustard, rosemary, salt and pepper together in a small bowl. Coat both sides of the racks with the mixture. Pat on a layer of matzoh meal."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Roast about 20 minutes for rare. Let stand five minutes."}]}