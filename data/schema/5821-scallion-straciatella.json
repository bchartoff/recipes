{"@context": "http://schema.org", "@type": "Recipe", "name": "Scallion Straciatella", "description": "", "author": {"@type": "Person", "name": "Florence Fabricant"}, "image": null, "totalTime": "PT10M", "recipeYield": "4 servings", "recipeCuisine": "italian", "recipeCategory": "soups and stews", "keywords": "appetizer, chicken stock, scallion", "aggregateRating": null, "nutrition": {"@context": "http://schema.org", "@type": "NutritionInformation", "calories": 219, "unsaturatedFatContent": "6 grams", "carbohydrateContent": "11 grams", "cholesterolContent": null, "fatContent": "13 grams", "fiberContent": "1 gram", "proteinContent": "13 grams", "saturatedFatContent": "7 grams", "sodiumContent": "526 milligrams", "sugarContent": "5 grams", "transFatContent": "0 grams"}, "recipeIngredient": ["1 1/2 cups scallions, finely chopped (about one bunch)", "2 tablespoons butter", "4 cups well-flavored chicken stock", "2 eggs", "1 tablespoon water", "3 tablespoons freshly grated parmesan cheese"], "recipeInstructions": [{"@context": "http://schema.org", "@type": "HowToStep", "text": "In a two-quart saucepan sautee scallions in butter until tender but still bright green. Add chicken stock and simmer for five minutes."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Beat eggs with water."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Bring soup to a simmer and stir it with a fork while allowing the egg mixture to dribble into the soup slowly."}, {"@context": "http://schema.org", "@type": "HowToStep", "text": "Lower heat, taste for seasoning and serve with grated cheese dusted on top of each portion."}]}